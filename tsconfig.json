{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "strict": false,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "allowJs": false,
    "jsx": "react-native",
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js",
    "**/*.js",
    "**/*.jsx"
  ]
}
